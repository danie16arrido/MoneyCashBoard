<h2>Create a new category</h2>
<%= erb :"categories/mypartial" %>
<section>
<form action='/categories' method="POST">
  <label for="l_name">Category Name:</label>
  <input type="text" id="l_name" name="name"/>
  <input type="hidden" id="l_m_category" name="master_category_id" value="<%= @master_category.id %>"/>

  <input type="submit" value="Add Category">
</form>
</section>
